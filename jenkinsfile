pipeline {
  agent {label 'java'}
  stages {
    stage('Stage 1') {
      steps {
        git url: 'https://github.com/bhargavianand/SampleMaven.git'
      }
    }
    stage('Compile-Package') {
      steps {
        def mvnHome = tool name: 'maven3.5.2', type: 'maven'
        sh "${mvnHome}/bin/mvn package"
      }
    }
    stage('Docker deploy'){
      steps {
        sh 'docker pull hello-world' 
      } 
    }
  }
}
